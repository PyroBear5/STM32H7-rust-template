{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "${defaultBuildTask}",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "Debug",
      "flashingConfig": {
        "flashingEnabled": true,
        // "haltAfterReset": true
      },
      "chip": "STM32H753ZI",
      "coreConfigs": [
        {
          "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/rust-ping-pong-nucleo",
          "coreIndex": 0,
          "rttEnabled": true
        }
      ],
      "env": {
        "RUST_LOG": "info"
      },
      "consoleLogLevel": "Console"
    }
  ]
}
